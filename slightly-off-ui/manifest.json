{
  "manifest_version": 3,
  "name": "Slightly-off UI",
  "version": "1.0.0",
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["slightlyOffUI.js"]
    }
  ],
  "action": {},
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}
